{{{

  "framework": "nextjs"

}  "framework": "nextjs",  "version": 2,

  "buildCommand": "npm run build"  "framework": "nextjs",

}  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://web-production-0ed11.up.railway.app/$1"
    },
    {
      "source": "/ws/(.*)",
      "destination": "https://web-production-0ed11.up.railway.app/ws/$1"
    },
    {
      "source": "/docs",
      "destination": "https://web-production-0ed11.up.railway.app/docs"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
