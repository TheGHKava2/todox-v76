{
  "version": 2,
  "name": "todox-v76",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next",
      "config": {
        "rootDirectory": "web"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "web/app/**": {
      "maxDuration": 30
    }
  }
}